{
    "info": {
        "name": "SynapticCare+ API",
        "description": "AI-Powered Medical Health Assistant API",
        "version": "1.0.0",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Authentication",
            "item": [
                {
                    "name": "Signup",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"securePassword123\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/auth/signup",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "auth",
                                "signup"
                            ]
                        }
                    }
                },
                {
                    "name": "Login",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"email\": \"john@example.com\",\n  \"password\": \"securePassword123\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/auth/login",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "auth",
                                "login"
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "Reports",
            "item": [
                {
                    "name": "Upload Report",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}"
                            }
                        ],
                        "body": {
                            "mode": "formdata",
                            "formdata": [
                                {
                                    "key": "file",
                                    "type": "file",
                                    "src": ""
                                }
                            ]
                        },
                        "url": {
                            "raw": "{{base_url}}/reports",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "reports"
                            ]
                        }
                    }
                },
                {
                    "name": "Get All Reports",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/reports?page=1&limit=10",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "reports"
                            ],
                            "query": [
                                {
                                    "key": "page",
                                    "value": "1"
                                },
                                {
                                    "key": "limit",
                                    "value": "10"
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Get Report by ID",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/reports/{{report_id}}",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "reports",
                                "{{report_id}}"
                            ]
                        }
                    }
                },
                {
                    "name": "Delete Report",
                    "request": {
                        "method": "DELETE",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/reports/{{report_id}}",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "reports",
                                "{{report_id}}"
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "AI Assistant",
            "item": [
                {
                    "name": "Send Chat Message",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}"
                            },
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"message\": \"What does my latest blood test mean?\",\n  \"reportId\": \"clx9876543210\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/assistant/chat",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "assistant",
                                "chat"
                            ]
                        }
                    }
                },
                {
                    "name": "Get Chat History",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/assistant/history?limit=50",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "assistant",
                                "history"
                            ],
                            "query": [
                                {
                                    "key": "limit",
                                    "value": "50"
                                }
                            ]
                        }
                    }
                }
            ]
        }
    ],
    "variable": [
        {
            "key": "base_url",
            "value": "http://localhost:5050/api/v1",
            "type": "string"
        },
        {
            "key": "token",
            "value": "",
            "type": "string"
        },
        {
            "key": "report_id",
            "value": "",
            "type": "string"
        }
    ]
}